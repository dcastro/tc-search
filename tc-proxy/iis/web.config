<configuration>
  <system.webServer>

    <iisnode enableXFF="true" />

    <handlers>
      <add name="iisnode" path="main.js" verb="*" modules="iisnode" />
    </handlers>

    <rewrite>
      <rules>
        <rule name="myapp">
          <match url=".*" />
          <action type="Rewrite" url="main.js" />
        </rule>
      </rules>
    </rewrite>
    
  </system.webServer>
</configuration>
